laceholder='sort by price' options={price} onChange={handleChange}/>\n    <h4>Filter</h4>\n        <Select placeholder='filter by category' options={category} onChange={handleChange}/>\n    </div>\n    <figure className='features'>\n        {filteredList.map((element, index) => (\n          <Item {...element} key={index} />\n        ))}\n    </figure>\n   </>\n  )\n}\n\nexport default FilterPrice\n","/Users/josh/Documents/projects/ecom-react-app copy/client/src/useForm.js",[],["98","99"],{"ruleId":"100","severity":1,"message":"101","line":1,"column":15,"nodeType":"102","messageId":"103","endLine":1,"endColumn":23},{"ruleId":"100","severity":1,"message":"104","line":1,"column":24,"nodeType":"102","messageId":"103","endLine":1,"endColumn":33},{"ruleId":"100","severity":1,"message":"105","line":2,"column":9,"nodeType":"102","messageId":"103","endLine":2,"endColumn":13},{"ruleId":"100","severity":1,"message":"106","line":5,"column":8,"nodeType":"102","messageId":"103","endLine":5,"endColumn":13},{"ruleId":"107","severity":1,"message":"108","line":16,"column":52,"nodeType":"109","endLine":16,"endColumn":64},{"ruleId":"107","severity":1,"message":"108","line":17,"column":56,"nodeType":"109","endLine":17,"endColumn":68},{"ruleId":"107","severity":1,"message":"108","line":19,"column":59,"nodeType":"109","endLine":19,"endColumn":74},{"ruleId":"107","severity":1,"message":"108","line":21,"column":58,"nodeType":"109","endLine":21,"endColumn":70},{"ruleId":"107","severity":1,"message":"108","line":15,"column":13,"nodeType":"109","endLine":15,"endColumn":43},{"ruleId":"107","severity":1,"message":"108","line":18,"column":13,"nodeType":"109","endLine":18,"endColumn":45},{"ruleId":"107","severity":1,"message":"110","line":15,"column":13,"nodeType":"109","endLine":15,"endColumn":34},{"ruleId":"111","severity":1,"message":"112","line":15,"column":34,"nodeType":"109","endLine":15,"endColumn":52},{"ruleId":"107","severity":1,"message":"110","line":18,"column":13,"nodeType":"109","endL